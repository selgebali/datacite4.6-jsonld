        "Dataset": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Dataset",
        "Dissertation": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Dissertation",
        "Event": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Event",
        "Image": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Image",
