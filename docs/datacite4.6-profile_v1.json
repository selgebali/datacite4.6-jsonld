        "ProjectManager": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#ProjectManager",
        "ProjectMember": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#ProjectMember",
        "RegistrationAgency": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#RegistrationAgency",
        "RegistrationAuthority": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#RegistrationAuthority",
        "RelatedPerson": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#RelatedPerson",
        "ResearchGroup": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#ResearchGroup",
        "RightsHolder": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#RightsHolder",
        "Researcher": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#Researcher",
        "Sponsor": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#Sponsor",
        "Supervisor": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#Supervisor",
        "Translator": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#Translator",
        "WorkPackageLeader": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#WorkPackageLeader"
      },
      "enumSchema": {
        "type": "string",
        "enum": [
          "ContactPerson", "DataCollector", "DataCurator", "DataManager",
          "Distributor", "Editor", "HostingInstitution", "Other",
          "Producer", "ProjectLeader", "ProjectManager", "ProjectMember",
          "RegistrationAgency", "RegistrationAuthority", "RelatedPerson",
          "ResearchGroup", "RightsHolder", "Researcher", "Sponsor",
          "Supervisor", "Translator", "WorkPackageLeader"
        ]
      }
    },
    "dateType": {
      "enum": [
        "Accepted", "Available", "Collected", "Copyrighted", "Coverage",
        "Created", "Issued", "Other", "Submitted", "Updated", "Valid", "Withdrawn"
      ],
      "iriMap": {
        "Accepted": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Accepted",
        "Available": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Available",
        "Collected": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Collected",
        "Copyrighted": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Copyrighted",
        "Coverage": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Coverage",
        "Created": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Created",
        "Issued": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Issued",
        "Other": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Other",
        "Submitted": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Submitted",
        "Updated": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Updated",
        "Valid": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Valid",
        "Withdrawn": "https://selgebali.github.io/datacite4.6-jsonld/dateTypes.jsonld#Withdrawn"
      },
      "enumSchema": {
        "type": "string",
        "enum": [
          "Accepted", "Available", "Collected", "Copyrighted", "Coverage",
