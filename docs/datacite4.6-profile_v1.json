        "OutputManagementPlan": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#OutputManagementPlan",
        "PeerReview": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#PeerReview",
        "PhysicalObject": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#PhysicalObject",
        "Preprint": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Preprint",
        "Project": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Project",
        "Report": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Report",
        "Service": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Service",
        "Software": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Software",
        "Sound": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Sound",
        "Standard": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Standard",
        "StudyRegistration": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#StudyRegistration",
        "Text": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Text",
        "Workflow": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Workflow",
        "Other": "https://selgebali.github.io/datacite4.6-jsonld/resourceTypeGeneral.jsonld#Other"
      },
      "enumSchema": {
        "type": "string",
        "enum": [
          "Audiovisual", "Award", "Book", "BookChapter", "Collection",
          "ComputationalNotebook", "ConferencePaper", "ConferenceProceeding",
          "DataPaper", "Dataset", "Dissertation", "Event", "Image",
          "InteractiveResource", "Instrument", "Journal", "JournalArticle",
          "Model", "OutputManagementPlan", "PeerReview", "PhysicalObject",
          "Preprint", "Project", "Report", "Service", "Software", "Sound",
          "Standard", "StudyRegistration", "Text", "Workflow", "Other"
        ]
      }
    },
    "descriptionType": {
      "enum": ["Abstract", "Methods", "SeriesInformation", "TableOfContents", "TechnicalInfo", "Other"],
      "iriMap": {
        "Abstract": "https://selgebali.github.io/datacite4.6-jsonld/descriptionTypes.jsonld#Abstract",
        "Methods": "https://selgebali.github.io/datacite4.6-jsonld/descriptionTypes.jsonld#Methods",
        "SeriesInformation": "https://selgebali.github.io/datacite4.6-jsonld/descriptionTypes.jsonld#SeriesInformation",
        "TableOfContents": "https://selgebali.github.io/datacite4.6-jsonld/descriptionTypes.jsonld#TableOfContents",
        "TechnicalInfo": "https://selgebali.github.io/datacite4.6-jsonld/descriptionTypes.jsonld#TechnicalInfo",
        "Other": "https://selgebali.github.io/datacite4.6-jsonld/descriptionTypes.jsonld#Other"
      },
      "enumSchema": { "type": "string", "enum": ["Abstract", "Methods", "SeriesInformation", "TableOfContents", "TechnicalInfo", "Other"] }
    },
    "contributorType": {
      "enum": [
        "ContactPerson", "DataCollector", "DataCurator", "DataManager",
        "Distributor", "Editor", "HostingInstitution", "Other",
        "Producer", "ProjectLeader", "ProjectManager", "ProjectMember",
        "RegistrationAgency", "RegistrationAuthority", "RelatedPerson",
        "ResearchGroup", "RightsHolder", "Researcher", "Sponsor",
        "Supervisor", "Translator", "WorkPackageLeader"
      ],
      "iriMap": {
        "ContactPerson": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#ContactPerson",
        "DataCollector": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#DataCollector",
        "DataCurator": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#DataCurator",
        "DataManager": "https://selgebali.github.io/datacite4.6-jsonld/contributorTypes.jsonld#DataManager",
