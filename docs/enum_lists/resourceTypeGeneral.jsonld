  "@graph": [
    {
      "@id": "datacite:ResourceTypeGeneralScheme",
      "@type": "skos:ConceptScheme",
      "skos:prefLabel": "Resource Type General",
      "skos:definition": "Controlled list of general resource types as defined in the DataCite Metadata Schema (4.6)."
    },

    {"@id":"datacite:Audiovisual","@type":"skos:Concept","skos:prefLabel":"Audiovisual","skos:definition":"A resource that is meant to be seen and heard (e.g., videos, animations, films, screencasts).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Award","@type":"skos:Concept","skos:prefLabel":"Award","skos:definition":"A funding instrument such as a grant, fellowship, or prize for which a DOI may be registered.","skos:inScheme":"datacite:ResourceTypeGeneralScheme","skos:note":"Added in 4.6."},
    {"@id":"datacite:Book","@type":"skos:Concept","skos:prefLabel":"Book","skos:definition":"A monograph, typically bound and consisting of multiple pages.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:BookChapter","@type":"skos:Concept","skos:prefLabel":"BookChapter","skos:definition":"A defined section of a book, usually with its own title or chapter number.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Collection","@type":"skos:Concept","skos:prefLabel":"Collection","skos:definition":"An aggregation of resources that may be curated together.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:ComputationalNotebook","@type":"skos:Concept","skos:prefLabel":"ComputationalNotebook","skos:definition":"An executable narrative notebook combining code, results, and text (e.g., Jupyter, R Markdown).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:ConferencePaper","@type":"skos:Concept","skos:prefLabel":"ConferencePaper","skos:definition":"A paper presented at a conference or workshop.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:ConferenceProceeding","@type":"skos:Concept","skos:prefLabel":"ConferenceProceeding","skos:definition":"A published collection of papers from a conference or workshop.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:DataPaper","@type":"skos:Concept","skos:prefLabel":"DataPaper","skos:definition":"A scholarly publication describing a dataset without new analysis.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Dataset","@type":"skos:Concept","skos:prefLabel":"Dataset","skos:definition":"Data encoded in a defined structure (e.g., tabular data, spreadsheets, databases).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Dissertation","@type":"skos:Concept","skos:prefLabel":"Dissertation","skos:definition":"A thesis or dissertation submitted for an academic degree.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Event","@type":"skos:Concept","skos:prefLabel":"Event","skos:definition":"A non-persistent occurrence such as a conference, meeting, or performance.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Image","@type":"skos:Concept","skos:prefLabel":"Image","skos:definition":"A visual representation other than text (e.g., photograph, diagram, map).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:InteractiveResource","@type":"skos:Concept","skos:prefLabel":"InteractiveResource","skos:definition":"A resource requiring user interaction to be understood, executed, or experienced.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Instrument","@type":"skos:Concept","skos:prefLabel":"Instrument","skos:definition":"A device or piece of equipment designed to perform a specific function, often for data acquisition.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Journal","@type":"skos:Concept","skos:prefLabel":"Journal","skos:definition":"A serial publication issued in successive parts, usually having a consistent title.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:JournalArticle","@type":"skos:Concept","skos:prefLabel":"JournalArticle","skos:definition":"An article published in a journal or other serial publication.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Model","@type":"skos:Concept","skos:prefLabel":"Model","skos:definition":"An abstract, conceptual, computational, or physical representation of a system or process.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:OutputManagementPlan","@type":"skos:Concept","skos:prefLabel":"OutputManagementPlan","skos:definition":"A plan for managing outputs (e.g., data or software) over their lifecycle.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:PeerReview","@type":"skos:Concept","skos:prefLabel":"PeerReview","skos:definition":"A review report evaluating the quality or validity of another work.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:PhysicalObject","@type":"skos:Concept","skos:prefLabel":"PhysicalObject","skos:definition":"A tangible, three-dimensional object (e.g., specimens, artifacts, samples).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Preprint","@type":"skos:Concept","skos:prefLabel":"Preprint","skos:definition":"A manuscript made publicly available prior to formal peer review and publication.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Project","@type":"skos:Concept","skos:prefLabel":"Project","skos:definition":"A coordinated set of activities with defined goals, duration, and participants (e.g., RAiD-identified projects).","skos:inScheme":"datacite:ResourceTypeGeneralScheme","skos:note":"Added in 4.6."},
    {"@id":"datacite:Report","@type":"skos:Concept","skos:prefLabel":"Report","skos:definition":"A formal report or technical document that communicates information or findings.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Service","@type":"skos:Concept","skos:prefLabel":"Service","skos:definition":"An organized system offering functions to end users (e.g., data management or preservation services).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Software","@type":"skos:Concept","skos:prefLabel":"Software","skos:definition":"Computer programs, code, scripts, or packages in source or binary form.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Sound","@type":"skos:Concept","skos:prefLabel":"Sound","skos:definition":"A resource primarily intended to be heard (e.g., audio recordings, music, spoken word).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Standard","@type":"skos:Concept","skos:prefLabel":"Standard","skos:definition":"A technical specification or standard (e.g., protocol, guideline, specification document).","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:StudyRegistration","@type":"skos:Concept","skos:prefLabel":"StudyRegistration","skos:definition":"A preregistration or registration record describing a study plan.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Text","@type":"skos:Concept","skos:prefLabel":"Text","skos:definition":"A primarily textual resource not covered by a more specific textual type in this list.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Workflow","@type":"skos:Concept","skos:prefLabel":"Workflow","skos:definition":"A defined sequence of steps or processes, often executable, used to perform a task.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"},
    {"@id":"datacite:Other","@type":"skos:Concept","skos:prefLabel":"Other","skos:definition":"A resource type not represented by any other category in this list.","skos:inScheme":"datacite:ResourceTypeGeneralScheme"}
  ]
}
