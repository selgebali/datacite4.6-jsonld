            "additionalProperties": false
          },
          "geoLocationBox": {
            "type": "object",
            "description": "The spatial limits of a box.",
            "properties": {
              "westBoundLongitude": { "type": "number", "description": "Western longitude of the box." },
              "eastBoundLongitude": { "type": "number", "description": "Eastern longitude of the box." },
              "southBoundLatitude": { "type": "number", "description": "Southern latitude of the box." },
              "northBoundLatitude": { "type": "number", "description": "Northern latitude of the box." }
            },
            "required": ["westBoundLongitude", "eastBoundLongitude", "southBoundLatitude", "northBoundLatitude"],
            "additionalProperties": false
          },
          "geoLocationPolygon": {
            "type": "array",
            "description": "A drawn polygon area, defined by a set of points and lines connecting the points in a closed chain.",
            "items": {
              "type": "object",
              "properties": {
                "polygonPoint": {
                  "type": "array",
                  "description": "A list of points that make up the polygon.",
                  "items": {
                    "type": "object",
                    "properties": {
                      "pointLatitude": { "type": "number", "description": "Latitude of the polygon point." },
                      "pointLongitude": { "type": "number", "description": "Longitude of the polygon point." }
                    },
                    "required": ["pointLatitude", "pointLongitude"],
                    "additionalProperties": false
                  }
                },
                "inPolygonPoint": {
                  "type": "object",
                  "description": "A point inside the polygon.",
                  "properties": {
                    "pointLatitude": { "type": "number", "description": "Latitude of the in-polygon point." },
                    "pointLongitude": { "type": "number", "description": "Longitude of the in-polygon point." }
                  },
                  "required": ["pointLatitude", "pointLongitude"],
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            }
          }
        },
